inicio funcoes
inicio funcao soma_numeros parametros: a:inteiro, b:inteiro, retorne:inteiro
    soma_ab = a + b
    retorne soma_ab
fim funcao
fim funcoes

inicio codigo principal

a = 2
b = 10000
resultado = soma_numeros(a, b)
se resultado > 10 entao
    escreva(resultado)
fim se

se a == b entao
    escreva("a igual a b")
senao se a < b entao
    escreva("a menor que b")
senao
    escreva("b maior que a")
fim se

x = 0
enquanto x < 10 faca
    incremente x
    escreva(x)
fim enquanto

lista numeros = [1, 2, 3, 4, 5]

para i entre numeros[0] e numeros[4] faca
    escreva(i)
fim para

escreva(numeros[3])

a_string = "123"
b = 123

converta para inteiro a_string a

soma_ab_string = a + b

escreva(soma_ab_string)

fim codigo principal