inicio codigo principal
lista nomes = ["João", "Maria", "José", "Pedro", "Paulo", "Lucas", "Mateus", "Marcos", "Luciana", "Mariana", "Joaquim", "Joaquina", "Joaquino", "Joaquinete", "Joaquimzinho"]
quantidade_nomes = 15
x = 0
enquanto x < quantidade_nomes faca
    escreva(nomes[x])
    incremente x
fim enquanto



fim codigo principal